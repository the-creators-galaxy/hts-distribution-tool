{
  "name": "@creators-galaxy/token-distribution-tool",
  "version": "1.1.0",
  "description": "Creator's Galaxy Token Distribution Tool",
  "author": "Calaxy, Inc.",
  "license": "MIT",
  "private": false,
  "main": "index.js",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "electron .",
    "dist": "npm run build && electron-builder",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "format": "prettier --config .prettierrc.json src/**/*.ts test/**/*.ts --write",
    "test": "mocha -r ts-node/register -r dotenv/config test/**/*.spec.ts",
    "test:unit": "mocha -r ts-node/register -r dotenv/config test/unit/**/*.spec.ts",
    "test:integration": "mocha -r ts-node/register -r dotenv/config test/integration/**/*.spec.ts"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.0",
    "@tsconfig/svelte": "^3.0.0",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "chai": "^4.3.6",
    "electron": "^16.0.8",
    "electron-builder": "^22.14.5",
    "mocha": "^9.2.0",
    "rollup": "^2.66.1",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.46.3",
    "svelte-check": "^2.3.0",
    "svelte-preprocess": "^4.10.2",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@hashgraph/sdk": "^2.7.1",
    "csv-parse": "^5.0.4",
    "csv-stringify": "^6.0.5",
    "prettier": "^2.5.1",
    "sirv-cli": "^2.0.2"
  }
}
